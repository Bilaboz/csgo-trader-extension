{
  "port": 3000,
  "buildDirectory": "./build",
  "engine": {
    "launchOptions": {
      "args": ["--no-sandbox", "--disable-setuid-sandbox"],
      "product": "chrome",
      "headless": true,
      "ignoreHTTPSErrors": true
    },
    "gotoOptions": {
      "timeout": 0
    }
  },
  "routes": [
    "/",
    "/changelog/index.html",
    "/release-notes/index.html",
    "/group/index.html",
    "/prices/index.html",
    "/privacy/index.html",
    "/faq/index.html"
  ]
}